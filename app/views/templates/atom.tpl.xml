<feed xmlns="http://www.w3.org/2005/Atom">
	<generator>{{generator}}</generator>
	<title>{{provider}} - ATOM</title> 
	<subtitle>{{description}}</subtitle>
	<id>{{provider_url}}</id>
	<updated>{{build_date}}</updated>
	<link rel="self" type="application/atom+xml" href="{{provider_url}}feed?output=atom" />
	<author>
		<name>{{authorName}}</name>
		<email>{{authorEmail}}</email>
	</author>
	<rights>&amp;copy;2014 {{provider}}</rights>
	
	{{#each entries}}
	<entry>
		<id>{{id}}</id>
		<title>{{title}}</title>
		<link href="{{link}}" />
		<image>{{image}}</image>
		<author>{{author}}</author>
		<summary>{{description}}</summary>
		<content type="html">
			{{content}}
		</content>
		<updated>{{date}}</updated>
	 </entry>
	 {{/each}}
</feed>
